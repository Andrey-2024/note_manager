# Создание списка для хранения информации о заметке
note = []

# Сбор данных от пользователя
note.append(input("Введите имя пользователя: "))
note.append(input("Введите содержание заметки: "))
note.append(input("Введите статус заметки (например, 'В работе', 'Завершено'): "))
note.append(input("Введите дату создания (в формате YYYY-MM-DD): "))
note.append(input("Введите дату изменения (в формате YYYY-MM-DD): "))

# Добавление заголовков в вложенный список
note.append([
    input("Введите заголовок 1: "),
    input("Введите заголовок 2: ")
])

# Вывод собранных данных
print("\nИнформация о заметке:")
print(f"Имя пользователя: {note[0]}")
print(f"Содержание заметки: {note[1]}")
print(f"Статус: {note[2]}")
print(f"Дата создания: {note[3]}")
print(f"Дата изменения: {note[4]}")
print(f"Заголовки: {note[5]}")
